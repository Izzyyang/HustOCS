<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>hustocsdataserver</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
        <servlet-name>UploadFileAction</servlet-name>
        <servlet-class>action.UpldFileServlet</servlet-class>
    </servlet>
  <servlet>
    <servlet-name>DowdFileServlet</servlet-name>
    <servlet-class>action.DowdFileServlet</servlet-class>
  </servlet>

    <servlet-mapping>
    	<servlet-name>UploadFileAction</servlet-name>
    	<url-pattern>/UploadFileAction</url-pattern>
    </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DowdFileServlet</servlet-name>
    <url-pattern>/DowdFileAction</url-pattern>
  </servlet-mapping>
</web-app>